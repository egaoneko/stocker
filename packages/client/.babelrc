{
  "env": {
    "development": {
      "compact": false,
      "presets": ["next/babel"]
    },
    "production": {
      "compact": false,
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [
        ["next/babel", { "preset-env": { "modules": "commonjs" } }]
      ]
    }
  },
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["inline-import", { "extensions": [".css"] }],
    ["import", { "libraryName": "antd" }],
    ["emotion"]
  ]
}
